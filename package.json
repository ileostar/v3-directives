{
  "name": "@ileostar/v3-directives",
  "version": "0.0.4",
  "packageManager": "pnpm@8.15.1",
  "description": "Custom Vue3 directives",
  "author": "ileostar <hi@leostar.top> (https://github.com/ileostar)",
  "license": "MIT",
  "homepage": "https://github.com/antfu/eslint-config",
  "bugs": {
    "url": "https://github.com/ileostar/v3-directives/issues"
  },
  "keywords": [
    "v3",
    "directives",
    "vue3"
  ],
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch & eslint-flat-config-viewer",
    "start": "esno src/index.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint src/directives/**/*.ts",
    "fix": "eslint src/**/*.*  --fix",
    "typecheck": "tsc --noEmit",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "release": "bumpp --commit --push --tag && npm publish"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.4",
    "@vitest/ui": "^1.2.2",
    "bumpp": "^9.3.0",
    "eslint": "^8.56.0",
    "esno": "^4.0.0",
    "lint-staged": "^15.2.2",
    "simple-git-hooks": "^2.9.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitepress": "1.0.0-rc.41",
    "vitest": "^1.2.2"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
